template<class T, class U> auto min(const T& a, const U& b) -> typename std::common_type<T, U>::type { return b < a ? b : a; }
template<class T, class U> auto max(const T& a, const U& b) -> typename std::common_type<T, U>::type { return b > a ? b : a; }
template<class T, class U> bool chmin(T& a, const U& b) { return b < a ? a = b, 1 : 0; }
template<class T, class U> bool chmax(T& a, const U& b) { return a < b ? a = b, 1 : 0; }
template<class T> int LB(vector<T> &v,T a){return lower_bound(v.begin(),v.end(),a)-v.begin();}
template<class T> int UB(vector<T> &v,T a){return upper_bound(v.begin(),v.end(),a)-v.begin();}
template<class T> T vec_min(const vector<T>& a) { assert(!a.empty()); return *min_element(all(a)); }
template<class T> T vec_max(const vector<T>& a) { assert(!a.empty()); return *max_element(all(a)); }
template<class T> int vec_min_idx(const vector<T>& a) { assert(!a.empty()); return int(min_element(all(a)) - a.begin()); }
template<class T> int vec_max_idx(const vector<T>& a) { assert(!a.empty()); return int(max_element(all(a)) - a.begin()); }
template<class T> T vec_sum(const vector<T>& a) { return accumulate(all(a), T(0)); }

