    template<typename... Args>
    static void check(bool condition, const char* cond_str, int line, Args&&... args) {
        if (!condition) {
            cerr << "Line " << line << ": Assertion failed [" << cond_str << "]";
            ((cerr << ": " << args), ...);
            cerr << '\n';
            abort();
        }
    }
};
